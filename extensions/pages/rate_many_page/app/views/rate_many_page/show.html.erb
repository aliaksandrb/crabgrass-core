<ul id='sort_list' class='possibles plain'>
  <%= render @possibles %>
</ul>

<% if current_user.may? :edit, @page -%>
  <div id='new_possible'>
    <%= render 'rate_many_possibles/new' %>
  </div> 
<% end -%>

<%= sortable_element 'sort_list', handle: 'handle',
  url: page_items_url(page_id: @page, controller: :rate_many_page) %>
