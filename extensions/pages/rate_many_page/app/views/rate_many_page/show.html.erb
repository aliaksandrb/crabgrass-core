<ul id='sort_list' class='possibles plain'>
  <%= render @possibles %>
</ul>

<% if current_user.may? :edit, @page -%>
  <div id='new_possible'>
    <%= render 'rate_many_possibles/new' %>
  </div> 
<% end -%>

<%= sortable_element 'sort_list', handle: 'handle', url: sort_rate_many_possibles_url(page_id: @page) %>
