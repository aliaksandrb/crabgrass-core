- version = version_to_list
%tr{:class => cycle('odd', 'even')}
  - url = wiki_version_path(@wiki, version.version)
  %td= link_to I18n.t(:version_number, :version => version.version), url
  %td= link_to full_time(version.updated_at), url
  %td= link_to_user(version.user, :avatar => :xsmall) if version.user
