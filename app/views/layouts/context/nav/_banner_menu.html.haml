- if @context_navigation
  %nav#banner_nav
    %ul#banner_nav_ul
      - @context_navigation.each do |tab|
        - if tab.visible
          %li.tab
            - if tab.icon
              = link_to_active("<span class='text'>#{tab.label}</span><span class='mobi_icon #{tab.icon}_16'/>", tab.url, tab.active, :class => "tab")
            - else
              = link_to_active(tab.label, tab.url,  tab.active, :class => "tab")
